{
  "name": "@ichef/gypcrete",
  "version": "0.12.1",
  "description": "iCHEF web components library, built with React.",
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/iCHEF/gypcrete.git"
  },
  "contributors": [
    "CJies Tan (https://github.com/cjies)",
    "Teng-hao Chang (https://github.com/zhusee2)"
  ],
  "license": "Apache-2.0",
  "engines": {
    "node": ">= 6.9.1",
    "npm": ">= 3.10.10"
  },
  "scripts": {
    "start": "npm run storybook",
    "storybook": "BABEL_ENV=storybook start-storybook -p 8100 -c .storybook",
    "build": "npm run build:dist && npm run build:lib && npm run build:es5",
    "build:dist": "webpack --config ./configs/webpack.dist.js --color",
    "build:lib": "BABEL_ENV=lib babel src --out-dir lib",
    "build:es5": "BABEL_ENV=es5 babel src --out-dir es5",
    "build:storybook": "BABEL_ENV=storybook build-storybook -o ./public",
    "flow": "flow",
    "lint": "npm run flow && npm run lint:eslint && npm run lint:stylelint",
    "lint:eslint": "eslint --ignore-path=configs/.eslintignore ./ && echo \"eslint: no lint errors\"",
    "lint:stylelint": "stylelint **/*.scss && echo \"stylelint: no lint errors\"",
    "test": "jest ./src --config=configs/jest.config.json --coverage",
    "test:watch": "jest ./src --config=configs/jest.config.json --coverage --watch",
    "clean": "rm -rf ./dist ./lib ./es5 ./deploy ./public",
    "deploy": "npm run clean && npm run build && sh ./scripts/deploy.sh",
    "ghpages": "npm run clean && npm run build:storybook && sh ./scripts/ghpages.sh"
  },
  "peerDependencies": {
    "babel-runtime": "^6.18.0",
    "react": "^15.3.0",
    "react-dom": "^15.3.0"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^3.0.0",
    "@storybook/addon-info": "^3.0.0",
    "@storybook/addon-options": "3.0.0",
    "@storybook/react": "3.0.0",
    "autoprefixer": "^6.7.6",
    "babel-cli": "^6.23.0",
    "babel-core": "^6.23.1",
    "babel-eslint": "^7.1.1",
    "babel-jest": "^19.0.0",
    "babel-loader": "^6.3.2",
    "babel-plugin-module-resolver": "^2.5.0",
    "babel-plugin-strip-css-imports": "zhusee2/babel-plugin-strip-css-imports",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.2.1",
    "babel-preset-react": "^6.23.0",
    "babel-preset-stage-2": "^6.22.0",
    "css-loader": "^0.26.2",
    "enzyme": "^2.8.2",
    "eslint": "^3.17.1",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-config-ichef": "^1.1.4",
    "eslint-import-resolver-babel-module": "^3.0.0",
    "eslint-loader": "^1.6.3",
    "eslint-plugin-flowtype": "^2.32.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.0",
    "extract-text-webpack-plugin": "^2.0.0",
    "file-loader": "^0.10.1",
    "flow-bin": "^0.47.0",
    "jest": "^19.0.2",
    "jest-junit": "zhusee2/jest-junit#update_format",
    "node-sass": "^4.5.0",
    "postcss-loader": "^1.3.3",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-test-renderer": "^15.5.4",
    "sass-loader": "^6.0.2",
    "sinon": "^2.2.0",
    "style-loader": "^0.13.2",
    "stylelint": "^7.10.1",
    "stylelint-config-standard": "^16.0.0",
    "webpack": "^2.2.1"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "document-offset": "^1.0.4",
    "keycode": "^2.1.9",
    "prop-types": "^15.5.8",
    "react-flow-types": "^0.1.1"
  },
  "babel": {
    "extends": "./configs/.babelrc"
  },
  "eslintConfig": {
    "root": true,
    "extends": "./configs/.eslintrc.yml"
  },
  "stylelint": {
    "extends": "./configs/.stylelintrc.yml"
  },
  "jest-junit": {
    "output": "./coverage/junit.xml"
  }
}
